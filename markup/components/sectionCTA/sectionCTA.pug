mixin sectionCTA(data)
	section.section.section_pt.section_pb.section_bg-dark.section_bg-sign.section-cta#section-cta
		.section-cta__line
		.container
			.row.justify-content-center.text-center
				.col-lg-8.col-12
					each item in data.text
						p!=item
					.mt-xl-5.mt-4
						+button(data.button)