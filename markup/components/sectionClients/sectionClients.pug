mixin sectionClients(data)
	section.section.section_pt.section_pb.section-clients#section-clients
		.container
			.row.justify-content-center.text-center
				.col-md-6
					+sectionHeader(data.header)
		.container
			.row.align-items-center.text-center
				each item in data.items
					.col-md-3.col-sm-6.py-4
						+imgSet(item, 'png', '3x')